{
    "manifest_version": 3,
    "name": "Schema Fetcher",
    "version": "1.0",
    "description": "Fetches schema from a website and provides a search function for schema.org terms.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
      }
    
  }
